<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	

	
	
  <display-name></display-name>
  <servlet>
    <servlet-name>JQuerySearch1</servlet-name>
    <servlet-class>servlet.JQuerySearch1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>checkUserJQueryServlet</servlet-name>
    <servlet-class>servlet.checkUserJQueryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>checkUser</servlet-name>
    <servlet-class>servlet.checkUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>checkVericode</servlet-name>
    <servlet-class>servlet.checkVericode</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>userRegister</servlet-name>
    <servlet-class>servlet.userRegister</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>JQuerySearch1</servlet-name>
    <url-pattern>/JQuerySearch1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>checkUserJQueryServlet</servlet-name>
    <url-pattern>/checkUserJQueryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>checkUser</servlet-name>
    <url-pattern>/checkUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>checkVericode</servlet-name>
    <url-pattern>/checkVericode</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>userRegister</servlet-name>
    <url-pattern>/userRegister</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
